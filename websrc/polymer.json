{
	"shell": "start-lit-element.js",
	"entrypoint": "index.html",
	"fragments": ["search-element.js"],
	"npm": true,
	"moduleResolution": "node",
	"sources": ["start-lit-element.js", "manifest/**", "manifest.json", "package.json"],
	"extraDependencies": [
		"node_modules/@webcomponents/webcomponentsjs/**"
	],
	"builds": [{
		"bundle": true,
		"js": { 
			"minify": false,
			"compile": "es5",
			"transformModulesToAmd": true
		},
		"addServiceWorker": true,
		"addPushManifest": true
	}]
}
